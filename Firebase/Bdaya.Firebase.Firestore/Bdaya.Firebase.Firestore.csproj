<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<Nullable>enable</Nullable>

		<PackageId>Bdaya.Firebase.Firestore</PackageId>
		<Authors>ahmednfwela</Authors>
		<Company>Bdaya-Dev</Company>
		<PackageDescription>Helpers for firebase auth, includes AuthListener that listens to a Users collection and syncs it up with a local database</PackageDescription>
		<RepositoryUrl>https://github.com/Bdaya-Dev/Bdaya.Firebase</RepositoryUrl>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="FirebaseAdmin" Version="2.2.0" />
		<PackageReference Include="Google.Cloud.Firestore" Version="2.4.0" />
	</ItemGroup>

	<Import Project="../FirebaseShared.targets" />
</Project>
