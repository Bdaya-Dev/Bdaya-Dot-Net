<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<Nullable>enable</Nullable>

		<PackageId>Bdaya.Firebase.Auth</PackageId>		
		<Authors>ahmednfwela</Authors>
		<Company>Bdaya-Dev</Company>
		<PackageDescription>Helpers for using jwt tokens created by firebase auth</PackageDescription>
		<RepositoryUrl>https://github.com/Bdaya-Dev/Bdaya.Firebase</RepositoryUrl>
		<PackageIcon>icon.jpg</PackageIcon>
	</PropertyGroup>

	<ItemGroup>
		<None Include="icon.jpg" Pack="true" Visible="false" PackagePath="" />
		<PackageReference Include="Microsoft.AspNetCore.Authentication.JwtBearer" Version="5.0.8" />
	</ItemGroup>

	<Import Project="../Shared.targets" />

</Project>
